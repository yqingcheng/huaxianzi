</template>

<script>

  import wepy from 'wepy'
  export default class Icon extends wepy.component {
    props = {
      type: {
        type: [String, Object, Array],
        coerce: newVal => {
//            console.log(newVal)
            return newVal
        }
      },
      fontSize: {
        type: [String],
        coerce: newVal => newVal
      },
      color: {
        type: [String],
        coerce: newVal => newVal
      }
    }
    show(data = {}){
//        console.log(data)

        for(let i in data){
            this[i] = data[i]
            this.$apply()
        }
    }
  }

</script>
